{"components/products/add-to-cart":"<div id=\"add-to-cart-wrapper\" {{#unless product.can_purchase}}style=\"display: none\"{{/unless}}>\r\n    {{#if theme_settings.show_product_quantity_box}}\r\n        {{inject 'productQuantityErrorMessage'  (lang 'products.quantity_error_message')}}\r\n        <div class=\"form-field form-field--increments\">\r\n            <label class=\"form-label form-label--alternate\"\r\n                   for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n                <div class=\"form-increment\" data-quantity-change>\r\n                    <button class=\"button button--icon minus\" data-action=\"dec\">\r\n                        <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                        \r\n\r\n                    </button>\r\n                    <input class=\"form-input form-input--incrementTotal form-input--incrementTotalPdp\"\r\n                           id=\"qty[]\"\r\n                           name=\"qty[]\"\r\n                           type=\"tel\"\r\n                           value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                           data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                           data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                           min=\"1\"\r\n                           pattern=\"[0-9]*\"\r\n                           aria-live=\"polite\">\r\n                    <button class=\"button button--icon plus\" data-action=\"inc\">\r\n                        \r\n\r\n                    </button>\r\n                </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n    <div class=\"alertBox productAttributes-message\" style=\"display:none\">\r\n        <div class=\"alertBox-column alertBox-icon\">\r\n            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n        </div>\r\n        <p class=\"alertBox-column alertBox-message\"></p>\r\n    </div>\r\n    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n        <div class=\"form-action Buynowmobile\">\r\n            <button class=\"button button--primary add_to_cart_product quick-buy\" type=\"button\" value=\"Buy Now\">\r\n              Buy Now\r\n            </button>\r\n          <!-- <input class=\"button button--primary add_to_cart_product quick-buy\" type=\"button\" value=\"Buy Now\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\"> -->\r\n          <!-- <span class=\"product-status-message aria-description--hidden\">{{lang 'products.adding_to_cart'}} {{lang 'category.add_cart_announcement'}}</span> -->\r\n            <button\r\n                    id=\"form-action-addToCart\"\r\n                    \r\n                    class=\"button button--primary add_to_cart_product\"\r\n                    type=\"submit\" \r\n                    value=\"\"\r\n            ><span class=\"addTocartText\">{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\r\n                <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/add-to-cart-btn.png'}}\" class=\"img-fluid lazyload\" alt=\"add to cart button\" loading=\"lazy\" width=\"90px\" height=\"81px\">\r\n            </span>\r\n            <div id=\"countinnermain\" class=\"pdpCount\"></div>\r\n           </button>\r\n            <span class=\"product-status-message aria-description--hidden\">{{lang 'products.adding_to_cart'}} {{lang 'category.add_cart_announcement'}}</span>\r\n        </div>\r\n    {{/or}}\r\n</div>\r\n"}
