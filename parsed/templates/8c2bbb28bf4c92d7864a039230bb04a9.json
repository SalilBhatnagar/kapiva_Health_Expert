{"components/common/subscription-form":"<h3 class=\"footer-info-heading\">JOIN OUR MAILING LIST</h3>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                     \r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       required\r\n                >\r\n                \r\n                <button class=\"button button--primary form-prefixPostfix-button--postfix\"\r\n                type=\"submit\"\r\n                value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                <svg xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" preserveAspectRatio=\"xMidYMid\" viewBox=\"0 0 31.906 17.91\">\r\n                    <path d=\"M31.897,8.940 C31.897,8.940 31.897,8.941 31.897,8.941 C31.897,8.941 31.897,8.941 31.897,8.941 C31.897,9.071 31.871,9.200 31.820,9.322 C31.770,9.444 31.696,9.554 31.603,9.646 L23.628,17.606 C23.434,17.800 23.178,17.897 22.923,17.897 C22.668,17.897 22.413,17.800 22.218,17.606 C21.829,17.217 21.829,16.587 22.218,16.198 L28.493,9.937 L0.986,9.937 C0.436,9.937 -0.011,9.491 -0.011,8.941 C-0.011,8.392 0.436,7.946 0.986,7.946 L28.493,7.946 L22.218,1.684 C21.829,1.295 21.829,0.665 22.218,0.276 C22.608,-0.112 23.239,-0.112 23.62