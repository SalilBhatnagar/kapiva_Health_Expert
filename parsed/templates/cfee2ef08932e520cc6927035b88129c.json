tton>\r\n          <button aria-label=\"Next\" class=\"glider-nextss glider-button-next\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" preserveAspectRatio=\"xMidYMid\" width=\"17.906\" height=\"31.91\" viewBox=\"0 0 17.906 31.91\">\r\n                  <path d=\"M0.995,31.924 C0.730,31.924 0.466,31.818 0.270,31.608 C-0.106,31.207 -0.086,30.576 0.315,30.200 L15.456,15.974 L0.315,1.748 C-0.086,1.372 -0.106,0.741 0.270,0.340 C0.645,-0.063 1.275,-0.083 1.676,0.294 L17.590,15.247 C17.791,15.435 17.905,15.699 17.905,15.974 C17.905,16.250 17.791,16.513 17.590,16.702 L1.676,31.654 C1.484,31.834 1.239,31.924 0.995,31.924 Z\" class=\"cls-1\"></path>\r\n                </svg>\r\n          </button> \r\n      </div>\r\n      <div class=\"dots-testimonials\">\r\n        <div  class=\"dotsexpert glider-dots-main coman-dots\"></div>\r\n      </div>\r\n     \r\n    </div>\r\n    \r\n  </div>\r\n</div>\r\n</section>\r\n<!-- EXPERT SPEAK end end-->\r\n<!-- science behind section start -->\r\n<section class=\"sciencebehindsection\">\r\n  <div class=\"sciencebehindimagetop\">\r\n    <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/sciencebehindtopimageflower.png'}}\" class=\"desk lazyload\" alt=\"science behind flower image\" loading=\"lazy\" width=\"608px\" height=\"1273px\">\r\n  </div>\r\n  <div class=\"container\">\r\n    \r\n  </div>\r\n  <div class=\"sciencebehindimage\">\r\n    <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/sciencebehindflowernew.png'}}\" class=\"desk lazyload\" alt=\"science behind flower image\" loading=\"lazy\" width=\"837px\" height=\"873px\">\r\n  </div>\r\n</section>\r\n<!-- science behind section end -->\r\n\r\n<!-- KEY INGREDIENTS start -->\r\n<section class=\"keyincredientssection\">\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\"><span>KEY </span>INGREDIENTS</h3>\r\n      <div class=\"keyincredientswraper\">\r\n        <ul class=\"row\">\r\n          \r\n        </ul>\r\n      </div>\r\n  </div>\r\n</section>\r\n<!--KEY INGREDIENTS end  -->\r\n<!-- suitable for start -->\r\n<section class=\"suitablesection\">\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\">SUITABLE <span> FOR</span></h3>\r\n      <p class=\"suitableText\">Females who are looking to get chemical-free, natural & healthy\r\n        skin glow with the right nutrition.</p>\r\n  </div>\r\n</section>\r\n<!-- suitable for end  -->\r\n<!-- how to use section start -->\r\n<section class=\"Howtousedsection\">\r\n  <div class=\"howtouseflowertop\">\r\n    <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/howtousedeskflowertop.png'}}\" class=\"desk lazyload\" alt=\"customer speak flower image\" loading=\"lazy\" width=\"820px\" height=\"1308px\">\r\n  </div>\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\"> <span>HOW TO </span> USE</h3>\r\n    <div class=\"HowtousedMain\">\r\n       \r\n       \r\n    </div>\r\n  </div>\r\n  \r\n</section>\r\n\r\n<!-- how to use section end -->\r\n\r\n\r\n\r\n<!-- faq section start -->\r\n<section class=\"product-faqs ingredients-tab-section\" id=\"faqs\">\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\"><span> FAQs</span></h3>\r\n    <div class=\"accr-wrap\">\r\n      \r\n    </div>\r\n    <div class=\"buttonread\">\r\n      <a href=\"javascript:void(0);\" class=\"btnReadMore\" style=\"display:none;\">VIEW  MORE</a>\r\n  </div>\r\n  </div>\r\n</section> \r\n<!-- faq section end -->\r\n<!-- judgme review start -->\r\n<section class=\"customerReviewSection\">\r\n  <div class=\"container\">\r\n    <div id='judgeme_product_reviews' class='jdgm-widget jdgm-review-widget' data-product-title='{{product.title}}' data-id='{{product.id}}'></div>\r\n  </div>\r\n</section>\r\n<!-- judgme review end -->\r\n<!-- why kapiva start -->\r\n <section class=\"whykapivaProduct\">\r\n  <div class=\"whykapivaflower\">\r\n    <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/whykapivadeskflower.png'}}\" class=\"desk lazyload\" alt=\"Why kapiva image\" loading=\"lazy\" width=\"855px\" height=\"1158px\">\r\n    <img src=\"{{cdn 'assets/img/loading.svg'}}\" data-src=\"{{cdn 'webdav:images/pdppageimage/whykapivamobileflower.png'}}\" class=\"mobile lazyload\" alt=\"why flower image\" loading=\"lazy\" width=\"260px\" height=\"379px\">\r\n  </div>\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\">Why<span> Kapiva?</span></h3>\r\n    <div class=\"row\">\r\n     \r\n    </div>\r\n  </div>\r\n\r\n</section>\r\n\r\n<!-- why kapiva end -->\r\n\r\n\r\n<!-- product specification start -->\r\n <section class=\"productspecification\">\r\n  <div class=\"container\">\r\n    <h3 class=\"heading-homepage\">PRODUCT<span> SPECIFICATIONS</span></h3>\r\n  </div>\r\n\r\n  <div class=\"container New\">\r\n      <div class=\"ProductspecificationMain\">\r\n       \r\n      </div>\r\n  </div>\r\n</section> \r\n<!-- prodcut specification section end -->\r\n\r\n\r\n\r\n\r\n<input type=\"hidden\" name=\"product_id\" class=\"product_id\" value=\"{{product.id}}\" />\r\n<input type=\"hidden\" name=\"product_id\" class=\"productid\" value=\"{{product.id}}\" />\r\n<input type=\"hidden\" name=\"dataid\" class=\"dataid\" value=\"\" />\r\n<input type=\"hidden\" name=\"customer_id\" class=\"customer_id\" value=\"{{customer.id}}\" />\r\n<input type=\"hidden\" class=\"customer_data\" value=\"{{customer.name}}\">\r\n<input type=\"hidden\" class=\"Product_Id\" value=\"{{product.id}}\">\r\n<input type=\"hidden\" class=\"product_details\" price=\"\" sale_price=\"\" quantity=\"\" category_name=\"\" category_id=\"\">\r\n<input type=\"hidden\" class=\"stickey_data\" title=\"{{product.title}}\" id=\"{{product.id}}\"\r\n  sale_price=\"{{product.price.with_tax.formatted}}\" actual_price=\"{{product.price.non_sale_price_with_tax.formatted}}\"\r\n  price_val=\"{{product.price.non_sale_price_with_tax.value}}\" sku=\"{{product.sku}}\" category=\"{{product.category}}\">\r\n<script>\r\n\r\n \r\n\r\n    //coupon popup js\r\n    function copyAndClosePdp() {\r\n       // var copyText = document.getElementById(\"code-copy\");\r\n        var copyTextPdp = document.querySelectorAll(\".codeCopyPdp\")[0].textContent;\r\n        // copyText.select();\r\n       // console.log(copyTextPdp);\r\n        //copyText.setSelectionRange(0, 99999); / For mobile devices /\r\n        navigator.clipboard.writeText(copyTextPdp);\r\n\r\n        document.querySelectorAll(\".copyAndClose\")[0].textContent = \"Copied!\";\r\n\r\n    }\r\n    function copyCouponPdp() {\r\n        copyAndClose();\r\n    }\r\n    \r\n  \r\n\r\n//update the prcentage\r\nvar saletag = document.getElementsByClassName('form-label');\r\n    if (saletag.length > 0) {\r\n      var saleSavingTag = document.getElementsByClassName(\"saleSavingTag\");\r\n      let discountText = saleSavingTag[0].textContent;\r\n      document.querySelectorAll(\".form-label\").forEach(item => {\r\n            item.addEventListener('click', function (event) {\r\n        let discountText = item.textContent;\r\n        let str = discountText.includes(\"@\");\r\n        if(str == true){\r\n       \r\n        var splitUrl = discountText.split(\"@\");\r\n        if(splitUrl[1] != null && splitUrl[1] != undefined){\r\n         var discountCOunt = splitUrl[1].split(\"%\");\r\n        var saleSavingTag = document.getElementsByClassName(\"saleSavingTag\");\r\n        \r\n        if(saleSavingTag[0].classList.contains(\"hidediscount\") == true){\r\n          document.querySelectorAll(\".saleSavingTag\").forEach(item => {\r\n                item.classList.remove(\"hidediscount\");\r\n                if(item.classList.contains(\"sale\")){\r\n                item.classList.remove(\"sale\");\r\n                item.parentElement.classList.remove(\"newsaletag\");\r\n                if(item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0] != undefined) \r\n               item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0].classList.remove(\"newsaletag\");\r\n           \r\n                }\r\n        });\r\n        }\r\n           document.querySelectorAll(\".saleSavingTag\").forEach(item => {\r\n                 item.innerHTML = discountCOunt[0]+\"% off\";\r\n              //  item.innerHTML = \"(\"+splitUrl[1]+\")\";\r\n              if(item.classList.contains(\"sale\")){\r\n                item.classList.remove(\"sale\");\r\n                item.parentElement.classList.remove(\"newsaletag\");\r\n                if(item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0] != undefined) \r\n               item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0].classList.remove(\"newsaletag\");\r\n             \r\n              }\r\n           });\r\n        }else{\r\n         document.querySelectorAll(\".saleSavingTag\").forEach(item => {\r\n                item.classList.add(\"hidediscount\");\r\n                if(item.classList.contains(\"sale\") == false){\r\n                item.classList.add(\"sale\");\r\n                item.parentElement.classList.add(\"newsaletag\");\r\n                if(item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0] != undefined)\r\n              item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0].classList.add(\"newsaletag\");\r\n               \r\n                }\r\n        });\r\n      }\r\n        }else{\r\n          document.querySelectorAll(\".saleSavingTag\").forEach(item => {\r\n                item.classList.add(\"hidediscount\");\r\n                if(item.classList.contains(\"sale\") == false){\r\n                item.classList.add(\"sale\");   \r\n                item.parentElement.classList.add(\"newsaletag\");   \r\n              if(item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0] != undefined) \r\n              item.parentElement.parentElement.querySelectorAll(\".linePrice\")[0].classList.add(\"newsaletag\");\r\n              \r\n              }\r\n        }); \r\n        }\r\n    });\r\n});\r\n    }\r\n//end\r\n\r\n  \r\n</script>\r\n"}
