{"components/account/orders-list":"<h3 class=\"account-heading\">{{lang 'account.orders.heading' }}</h3>\r\n\r\n<table cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\r\n <tbody> \r\n   <tr class=\"account-listItem heading\">\r\n     <th align=\"left\" width=\"100\">ORDER</th> <th class=\"border-lft\" align=\"left\" width=\"100\">ITEMS</th> \r\n     <th class=\"border-lft\" align=\"left\" width=\"100\">TOTAL</th> <th class=\"border-lft\" align=\"left\" width=\"180\">ORDER PLACED</th> <th class=\"border-lft\" align=\"left\" width=\"180\">LAST UPDATE</th> <th class=\"border-lft\" align=\"left\" width=\"150\">TRACKING</th> <th class=\"border-lft\" align=\"left\" width=\"150\">STATUS</th> <th class=\"border-lft\" width=\"150\">CANCEL ORDER</th>\r\n    </tr>\r\n    </tr>\r\n    \r\n    {{#each orders}}\r\n    <tr class=\"account-listItem\">\r\n        <td align=\"left\" class=\"order\" number={{this.id}}>\r\n          <strong><a href=\"{{this.details_url}}\">{{lang 'account.orders.list.order_number' number=this.id}}</a></strong>\r\n        </td>\r\n        <td align=\"left\" class=\"item\">\r\n            {{lang 'account.orders.list.product_details' num_products=this.items.length cost=this.total.formatted }}\r\n        </td>\r\n        <td align=\"left\" class=\"total\">\r\n            {{lang 'account.orders.list.product_cost' cost=this.total.formatted }}\r\n        </td>\r\n        <td align=\"left\" class=\"place-order\">\r\n            <span>{{this.date}}</span>\r\n        </td>\r\n        <td align=\"left\" class=\"last-date-order\">\r\n         <span>{{this.last_update_date}}</span>\r\n        </td>\r\n        <td align=\"left\" class=\"tracking-link track_{{this.id}}\">\r\n         \r\n        </td>\r\n        <td align=\"left\" class=\"status\">\r\n              <h6 class=\"account-orderStatus-label order-{{this.status}}\">{{this.status}}</h6>\r\n                    \r\n                    {{#if ../settings.returns_enabled}}\r\n                        {{#if this.return_url}}\r\n                            <a class=\"account-orderStatus-action\" href=\"{{this.return_url}}\">\r\n                                {{lang 'account.orders.return_items' }}\r\n                            </a>\r\n                        {{/if}}\r\n                    {{/if}}\r\n        </td> \r\n        <td align=\"left\" class=\"cancle-link cancle_{{this.id}}\">\r\n         \r\n        </td>\r\n    </tr>\r\n    {{/each}}\r\n     \r\n</tbody>\r\n </table>\r\n\r\n <!--<div class=\"woot-widget-holder woot-elements--right woot--hide\"><iframe data-src=\"https://app.limechat.ai/widget?website_token=vUkiLx4cqqJVqRM5ocVvkEoc&amp;cw_conversation=eyJhbGciOiJIUzI1NiJ9.eyJzb3VyY2VfaWQiOiI3MTgxMzBmMi05MzcwLTQ3ZTItYTljZC0yYjIyM2E1ZjRlODkiLCJpbmJveF9pZCI6MTY1fQ.zMi3P7brGl_QJ5xp_ceFKesaG09DSGS-hSGrCsSNroE\" id=\"chatwoot_live_chat_widget\" src=\"https://app.limechat.ai/widget?website_token=vUkiLx4cqqJVqRM5ocVvkEoc&amp;cw_conversation=eyJhbGciOiJIUzI1NiJ9.eyJzb3VyY2VfaWQiOiI3MTgxMzBmMi05MzcwLTQ3ZTItYTljZC0yYjIyM2E1ZjRlODkiLCJpbmJveF9pZCI6MTY1fQ.zMi3P7brGl_QJ5xp_ceFKesaG09DSGS-hSGrCsSNroE\" style=\"\"></iframe></div>-->\r\n <!--<script>-->\r\n <!--  $(document).on(\"click\", \".cancel_button\", function(){-->\r\n <!--      $(\".woot-widget-holder\").removeClass(\"woot--hide\");-->\r\n <!--  })-->\r\n <!--</script>-->\r\n \r\n \r\n"}
